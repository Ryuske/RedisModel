{
    "name": "ryuske/redismodel",
    "type": "library",
    "description": "Use Redis as a primary data store.",
    "keywords": [
        "ryuske",
        "redis",
        "php",
        "model"
    ],
    "homepage": "https://github.com/Ryuske/RedisModel",
    "license": "MIT",
    "authors": [
        {
            "name": "Kenyon Haliwell",
            "email": "kenyon.jh@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "predis/predis": "^1.1",
        "illuminate/support": "5.2.*",
        "laravel/laravel": "5.2.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*"
    },
    "autoload": {
        "psr-4": {
            "Ryuske\\RedisModel\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "Ryuske\\RedisModel\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
